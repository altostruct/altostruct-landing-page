{
  "name": "new-site",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "scripts": {
    "prebuild": "node scripts/prebuild.mjs",
    "dev": "next dev",
    "build": "rm -rf out && npm run export && npm run postbuild",
    "build-simple": "npm run export",
    "start": "next start",
    "lint": "next lint",
    "serve": "serve out",
    "export": "next build && next export && npm run parse:images",
    "parse:images": "next-image-export-optimizer",
    "translate": "i18next './{pages,components}/**/*.{ts,tsx}'",
    "postinstall": "patch-package",
    "postbuild": "node scripts/postbuild.mjs && npm run sitemap",
    "sitemap": "sscli -b https://www.altostruct.com -r out -p '**,0.7' -c '**,weekly'"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.7.3",
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/poppins": "^4.5.10",
    "@mui/material": "^5.15.2",
    "@next/third-parties": "^14.2.4",
    "@react-three/drei": "^8.18.10",
    "@react-three/fiber": "7.0.26",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tweenjs/tween.js": "^18.6.4",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@use-gesture/react": "^10.2.22",
    "chart.js": "^3.9.1",
    "classnames": "^2.3.2",
    "emailjs-com": "^3.2.0",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.2",
    "framer-motion": "^10.12.10",
    "globe.gl": "^2.28.4",
    "i18next": "^22.0.5",
    "i18next-parser": "^7.0.3",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "next": "13.4.2",
    "next-image-export-optimizer": "^1.6.1",
    "react": "18.2.0",
    "react-aws-icons": "^1.2.1",
    "react-bootstrap": "^2.8.0",
    "react-calendly": "^4.1.1",
    "react-chartjs-2": "^4.3.1",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "18.2.0",
    "react-globe.gl": "^2.24.1",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^4.12.0",
    "react-measure": "^2.5.2",
    "react-responsive-carousel": "^3.2.23",
    "react-sizeme": "^3.0.2",
    "react-spring": "^9.5.5",
    "react-tagcloud": "^2.3.1",
    "react-type-animation": "^3.2.0",
    "react-use-gesture": "^9.1.3",
    "rsuite": "^5.38.0",
    "sass": "^1.56.1",
    "three": "^0.141.0",
    "typescript": "4.8.4",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.22",
    "@types/js-cookie": "^3.0.3",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-measure": "^2.0.8",
    "@types/react-tagcloud": "^1.1.7",
    "@types/three": "^0.141.0",
    "autoprefixer": "^10.4.13",
